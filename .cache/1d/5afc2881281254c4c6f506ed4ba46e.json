nheader .icon-close:hover {\n  font-size: 1.7rem;\n  color: crimson;\n  rotate: 180deg;\n}\n\n/* ========================\n    3- Modal == pop up\n=========================== */\nheader .fixed {\n  position: fixed;\n  inset: 0;\n  z-index: 11;\n  background-color: rgba(40, 40, 48, 0.91);\n\n  /* filter: blur(8px); */\n  backdrop-filter: blur(4px);\n}\n\n@keyframes mymove {\n  0% {\n    scale: 0;\n  }\n\n  60% {\n    scale: 1.2;\n  }\n\n  100% {\n    scale: 1;\n  }\n}\n\nheader .fixed ul.modal {\n  animation: mymove 0.77s 1;\n  width: 75%;\nmargin-inline: auto;\n\n\n  margin-top: 2rem;\n  border-radius: 1rem;\n  padding: 1rem 2rem;\n  background-color: var(--secondary);\n}\n\nheader .fixed ul.modal li {\n  border-bottom: 1px solid var(--border);\n  padding-bottom: 0.77rem;\n  padding-top: 0.5rem;\n}\n\nheader .fixed ul.modal li:first-child {\n  text-align: right;\n  border: none;\n  padding-bottom: 0;\n  padding-top: 0.2rem;\n  margin-bottom: -1rem;\n}\n\nheader .fixed ul.modal li:last-child {\n  border: none;\n}\n\nheader .fixed ul.modal li a {\n  font-size: 1rem;\n}\n\n/* =====================================\n  4- Responsive Design (media query)\n======================================== */\n@media (width < 700px) {\n  header.flex nav {\n    display: none;\n  }\n\n  header button.menu {\n    display: block;\n  }\n}\n","js":"var reloadCSS = require('_css_loader');\nmodule.hot.dispose(reloadCSS);\nmodule.hot.accept(reloadCSS);"},"sourceMaps":{"css":{"mappings":[{"source":"src/components/1-header/header.css","original":{"line":1,"column":0},"generated":{"line":1,"column":0}},{"source":"src/components/1-header/header.css","original":{"line":2,"column":0},"generated":{"line":2,"column":0}},{"source":"src/components/1-header/header.css","original":{"line":3,"column":0},"generated":{"line":3,"column":0}},{"source":"src/components/1-header/header.css","original":{"line":4,"column":0},"generated":{"line":4,"column":0}},{"source":"src/components/1-header/header.css","origina